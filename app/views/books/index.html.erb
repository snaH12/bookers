<h1>投稿一覧</h1>
<% @books.each do |book| %>
 <table>タイトル
 <span>
  <%= link_to book.title, book_path(book.id) %>
 </span>
 </table>
 
<% end %>

<h1>新規登録</h1>
<%= form_with model:@book do |f| %>
　<h4>タイトル</h4>
　<%= f.text_field:title %>
　<h4>感想</h4>
　<%= f.text_area:body %>
　<%= f.submit '投稿' %>
<% end %>